{
  "game_name": "PacMan",
  "version": "1.0.0",
  "url": "https://github.com/Jesse-Jumbo/pac-man_game",
  "game_params": [
    {
      "name": "game_mode",
      "verbose": "遊戲模式",
      "type": "str",
      "choices": [
        {
          "verbose": "SINGLE",
          "value": "SINGLE"
        },
        {
          "verbose": "BATTLE",
          "value": "BATTLE"
        }
      ],
      "help": "choose the type of game",
      "default": "SINGLE"
    },
    {
      "name": "map_no",
      "verbose": "地圖編號",
      "type": "int",
      "max": 11,
      "min": 1,
      "default": 2,
      "help": "set map number for load game scene"
    },
    {
      "name": "time_limit",
      "verbose": "時間限制",
      "type": "int",
      "max": 600,
      "min": 1,
      "default": 25,
      "help": "set the time number of your game level"
    },
    {
      "name": "sound",
      "verbose": "遊戲音效",
      "type": "str",
      "choices": [
        "on",
        "off"
      ],
      "help": "'off' can turn off the sound.",
      "default": "on"
    }
  ]
}
